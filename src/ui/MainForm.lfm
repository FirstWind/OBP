object MainForm: TMainForm
  Left = 200
  Height = 600
  Top = 120
  Width = 900
  Caption = 'OBP'
  OnCreate = FormCreate
  OnDestroy = FormDestroy
  object ContentPanel: TPanel
    Align = alClient
    Caption = ''
    TabOrder = 0
    object PageControlMain: TPageControl
      Align = alClient
      TabOrder = 0
      object TabSessions: TTabSheet
        Caption = 'Сессии'
        object LabelSessionsHint: TLabel
          Left = 16
          Top = 16
          Caption = 'Экран в разработке'
        end
      end
      object TabParticipants: TTabSheet
        Caption = 'Участники'
        object LabelParticipantsHint: TLabel
          Left = 16
          Top = 16
          Caption = 'Экран в разработке'
        end
      end
      object TabAssignments: TTabSheet
        Caption = 'Упражнения'
        object PanelAssignTop: TPanel
          Align = alTop
          Height = 200
          TabOrder = 0
          object LabelPack: TLabel
            Left = 8
            Top = 8
            Caption = 'Norms Pack:'
          end
          object EditPackPath: TEdit
            Left = 100
            Top = 4
            Width = 520
            TabOrder = 0
          end
          object BtnBrowsePack: TButton
            Left = 630
            Top = 2
            Width = 40
            Caption = '...'
            OnClick = BtnBrowsePackClick
            TabOrder = 1
          end
          object BtnLoadExercises: TButton
            Left = 680
            Top = 2
            Width = 170
            Caption = 'Загрузить упражнения'
            OnClick = BtnLoadExercisesClick
            TabOrder = 2
          end
          object LblCatalogInfo: TLabel
            Left = 100
            Top = 32
            Caption = 'Упражнения не загружены'
          end
          object LabelParticipant: TLabel
            Left = 8
            Top = 60
            Caption = 'ID участника:'
          end
          object EditParticipantId: TEdit
            Left = 100
            Top = 56
            Width = 120
            TabOrder = 3
          end
          object CheckCustomOrder: TCheckBox
            Left = 240
            Top = 56
            Caption = 'Кастомный порядок'
            TabOrder = 4
          end
          object LabelReason: TLabel
            Left = 8
            Top = 88
            Caption = 'Причина:'
          end
          object EditCustomReason: TEdit
            Left = 100
            Top = 84
            Width = 520
            TabOrder = 5
          end
          object LabelExercises: TLabel
            Left = 8
            Top = 116
            Caption = 'Упражнения (ID или ID:вариант, по строкам):'
          end
          object MemoExerciseIds: TMemo
            Left = 8
            Top = 136
            Width = 612
            Height = 56
            ScrollBars = ssVertical
            TabOrder = 6
          end
          object BtnCreateAssignment: TButton
            Left = 630
            Top = 136
            Width = 220
            Height = 56
            Caption = 'Создать назначение'
            OnClick = BtnCreateAssignmentClick
            TabOrder = 7
          end
        end
        object GridExercises: TStringGrid
          Align = alClient
          ColCount = 5
          FixedCols = 0
          RowCount = 2
          Options = [goFixedVertLine, goFixedHorzLine, goVertLine, goHorzLine, goRowSelect]
          TabOrder = 1
        end
      end
      object TabResults: TTabSheet
        Caption = 'Результаты'
        object LabelResultsHint: TLabel
          Left = 16
          Top = 16
          Caption = 'Экран в разработке'
        end
      end
      object TabImport: TTabSheet
        Caption = 'Импорт'
        object LabelImportHint: TLabel
          Left = 16
          Top = 16
          Caption = 'Экран в разработке'
        end
      end
      object TabReports: TTabSheet
        Caption = 'Отчёты'
        object LabelReportsHint: TLabel
          Left = 16
          Top = 16
          Caption = 'Экран в разработке'
        end
      end
    end
  end
  object StatusBar: TStatusBar
    Align = alBottom
    SimplePanel = True
    SimpleText = 'Подключено к серверу.'
  end
end
